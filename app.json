{
  "expo": {
    "name": "Parasol Fleet",
    "slug": "parasol-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/stph_72.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "parasol",
    "splash": {
      "image": "./assets/icons/stph_72.png",
      "resizeMode": "contain",
      "backgroundColor": "#FCD116"
    },
    "ios": {
      "bundleIdentifier": "com.safetravelph.parasol",
      "deploymentTarget": "12.0",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "For a better, experience, your device will need to use Location Accuracy",
        "NSLocationAlwaysUsageDescription": "For a better, experience, your device will need to use Location Accuracy",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "For a better, experience, your device will need to use Location Accuracy",
        "NSPhotoLibraryUsageDescription": "We need access to save files to your photo library",
        "NSPhotoLibraryAddUsageDescription": "We need access to save files to your photo library"
      },
      "config": {
        "googleMapsApiKey": "AIzaSyAceRPYDG5xfrwFzFPET1EsKhEkLXasKw0"
      },
      "supportsTablet": false
    },
    "android": {
      "package": "com.safetravelph.parasol",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAceRPYDG5xfrwFzFPET1EsKhEkLXasKw0"
        }
      },
      "permissions": [
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_BACKGROUND_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/stph_72.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/icons/stph_72.png"
    },
    "plugins": [
      "expo-router",
      "expo-sqlite"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "5958cfd3-998c-4a2a-96e6-f3790561425d"
      }
    }
  }
}
